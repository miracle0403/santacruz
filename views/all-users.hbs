 {{> spagenav}}
 
 <div class = "content">
 
 <div class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="card">
              <div class="card-header">
			   <h1 class="title text-center">ALL USERS</h1>
                
			  </div>
              <div class="card-body">
			 
               <div >
                  <table class="table">
                    <thead class=" text-primary">
                      <th>
                        FULL NAME
                      </th>
                      <th>
                        USERNAME
                      </th>
                      <th>
                        PHONE
                      </th>
                      <th>
                        EMAIL
                      </th>
					  <th>
                       RESPONSIBILITY
                      </th>
                    </thead>
                    <tbody>
					{{#if user}}
					{{#each user}}
                      <tr>
                        <td>
                          {{full_name}}
                        </td>
                        <td>
                          {{username}}
                        </td>
                        <td>
                          {{phone}}
                        </td>
                        <td>
                          {{email}}
                        </td>
						<td>
                          {{user_type}}
                        </td>
                      </tr>
					  {{/each}}
					  {{/if}}
					  </tbody>
					 </table>
					</div>
					
					
              </div>
            </div>
          </div>
        </div>
      </div>
 </div>